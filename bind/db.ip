;
; BIND reverse data file for local loopback interface
;
$TTL	604800
@	IN	SOA	abu.com. root.abu.com. (
			      2		; Serial
			 604800		; Refresh
			  86400		; Retry
			2419200		; Expire
			 604800 )	; Negative Cache TTL
;
@	IN	NS	ns.abu.com.
32	IN 	PTR ns.abu.com.
32	IN 	PTR abu.com.
32	IN 	PTR www.abu.com.
32	IN 	PTR ftp.abu.com.
32	IN 	PTR mail.abu.com.
32	IN 	PTR portal.abu.com.

