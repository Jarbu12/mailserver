;
; BIND data file for local loopback interface
;
$TTL	604800
@	IN	SOA	abu.com. root.abu.com. (
			      2		; Serial
			 604800		; Refresh
			  86400		; Retry
			2419200		; Expire
			 604800 )	; Negative Cache TTL
;
@		IN	NS	abu.com.
@		IN	A	192.168.1.32
@		IN	MX  2	mail.abu.com.
ns		IN	A	192.168.1.32
www		IN	A	192.168.1.32
ftp		IN	A	192.168.1.32
mail 	IN	A	192.168.1.32
portal	IN	A	192.168.1.32
